{
  "presets": ["next/babel"],
  "plugins": [
    "add-react-displayname",
    [
      "module-resolver", {
        "root": ["."],
        "alias": {
          "components": "./components",
          "constants": "./constants",
          "cosmos": "./cosmos",
          "css": "./css",
          "modules": "./modules",
          "services": "./services",
          "utils": "./utils"
        },
    }],
    [
      "wrap-in-js",
      {
        "extensions": ["css$", "scss$"]
      }
    ]
  ]
}
