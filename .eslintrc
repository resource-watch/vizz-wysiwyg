{
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "es6": true
  },
  "globals": {
    "L": true,
    "config": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "babel"
  ],
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  }
}
